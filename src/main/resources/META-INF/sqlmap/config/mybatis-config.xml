		<mapper resource="META-INF/sqlmap/mappers/fundingQuestion.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/fundingQuestionReply.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/notice.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/notification.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/qnA.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/reward.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/userPick.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/users.xml"/>
	</mappers>	

</configuration>