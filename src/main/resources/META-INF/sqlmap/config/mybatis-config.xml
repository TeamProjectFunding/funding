		<typeAlias type="com.tp.funding.dto.FundingQuestionReply" alias="FundingQuestionReply"/>
		<typeAlias type="com.tp.funding.dto.Notice" alias="Notice"/>
		<typeAlias type="com.tp.funding.dto.Notification" alias="Notification"/>
		<typeAlias type="com.tp.funding.dto.QnA" alias="QnA"/>
		<typeAlias type="com.tp.funding.dto.Reward" alias="Reward"/>
		<typeAlias type="com.tp.funding.dto.UserGrade" alias="UserGrade"/>
		<typeAlias type="com.tp.funding.dto.UserPick" alias="UserPick"/>
		<typeAlias type="com.tp.funding.dto.Users" alias="Users"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="META-INF/sqlmap/mappers/admin.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/company.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/event.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/eventPrize.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/eventReply.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/fgComments.xml"/>
